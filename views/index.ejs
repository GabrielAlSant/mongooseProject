<!DOCTYPE html>
<html>
  <head>
    <title></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h2>Usuários Cadastrados:</h2>
    <% if (users.length > 0) { %>
      <ul>
        <% users.forEach(function(user) { %>
          <li><strong>Nome:</strong> <%= user.name %> | <strong>Email:</strong> <%= user.email %></li>
        <% }) %>
      </ul>
    <% } else { %>
      <p>Nenhum usuário cadastrado.</p>
    <% } %>
  </body>
</html>
